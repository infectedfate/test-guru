<tr>
  <td> <%= test.title %> </td>
  <td> <%= test_level(test) %> </td>
  <td> <%= test.questions.count %> </td>
  <td> <%= link_to 'Вход', test_path(test) %> </td>
  <td> <%= link_to 'Редактировать', edit_test_path(test) %> </td>
  <td> <%= link_to 'Удалить',
                    test_path(test),
                    method: :delete,
                    data: { confirm: 'Are you shure?' } %>
  </td>
  <td> <%= button_to 'Начать', start_test_path(test) %> </td>

</tr>
